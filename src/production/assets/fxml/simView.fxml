<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>

<TabPane prefHeight="1080.0" side="RIGHT" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1">
   <tabs>
      <Tab text="Electrical">
         <content>
            <BorderPane>
               <top>
                  <ImageView fitHeight="150.0" fitWidth="150.0" pickOnBounds="true" preserveRatio="true" BorderPane.alignment="CENTER">
                     <image>
                        <Image url="@../image/bluetable.png" />
                     </image>
                  </ImageView>
               </top>
            </BorderPane>
         </content>
      </Tab>
      <Tab text="Temperature">
         <content>
            <BorderPane>
               <top>
                  <ImageView fitHeight="150.0" fitWidth="150.0" pickOnBounds="true" preserveRatio="true" BorderPane.alignment="CENTER">
                     <image>
                        <Image url="@../image/redtable.png" />
                     </image>
                  </ImageView>
               </top>
            </BorderPane>
         </content>
      </Tab>
   </tabs>
</TabPane>
